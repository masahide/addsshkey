{
  "name": "sample",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv dist --no-clear",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile dist/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile dist/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@primer/css": "^19.2.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "autoprefixer": "^10.4.2",
    "from": "^0.1.7",
    "import": "^0.0.6",
    "node-sass": "^7.0.1",
    "postcss": "^8.3.5",
    "postcss-url": "^10.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-bundle-html": "^0.2.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "smelte": "^1.1.8",
    "smui-theme": "^6.0.0-beta.13",
    "svelte": "^3.0.0",
    "svelte-dnd-action": "^0.9.14"
  },
  "dependencies": {
    "@github/details-dialog-element": "^3.1.3",
    "@primer/primitives": "^7.4.0",
    "sirv-cli": "^1.0.0",
    "svelte-material-ui": "^6.0.0-beta.13",
    "svelte-octicons": "^17.1.0",
    "svelte-table": "^0.3.2",
    "uuid": "^8.3.2"
  },
  "author": "YAMASAKI Masahide"
}
